                if game.Players:FindFirstChild(v201) then
                    game.Players[v201].Character.UpperTorso.CFrame = CFrame.new(0, 2000, 0)
                end
                
                wait(3.5)
                
                -- Reset effects
                game:GetService('TweenService'):Create(game.Lighting, TweenInfo.new(0.7), {ColorShift_Bottom = Color3.fromRGB(0,0,0)}):Play()
                game:GetService('TweenService'):Create(game.Lighting, TweenInfo.new(0.7), {ColorShift_Top = Color3.fromRGB(0,0,0)}):Play()
                game:GetService('TweenService'):Create(game.Lighting, TweenInfo.new(0.7), {Brightness = 1}):Play()
                game:GetService('TweenService'):Create(game.Lighting, TweenInfo.new(0.7), {Ambient = Color3.fromRGB(0,0,0)}):Play()
                
                _LocalPlayer2.Character.Humanoid.WalkSpeed = 16
            end
        end)

        -- Hado 90 Tool (del GUI)
        if _LocalPlayer2.Backpack:FindFirstChild('Hado 90') then
            _LocalPlayer2.Backpack:FindFirstChild('Hado 90'):Destroy()
        end
        task.wait(0.1)
        local hado90Tool = Instance.new('Tool')
        hado90Tool.RequiresHandle = false
        hado90Tool.Name = 'Hado 90'
        hado90Tool.Parent = _LocalPlayer2.Backpack
        hado90Tool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local v203 = tostring(_LocalPlayer2.Character.BodyEffects.Grabbed.Value)
                local player = game.Players:FindFirstChild(v203)
                
                if player and player.Character and player.Character:FindFirstChild('UpperTorso') then
                    -- Hado 90 sound
                    if _LocalPlayer2.Backpack:FindFirstChild('[Boombox]') then
                        _LocalPlayer2.Backpack['[Boombox]'].Parent = _LocalPlayer2.Character
                        _ReplicatedStorage.MainEvent:FireServer("Boombox", 4743237673)
                        _LocalPlayer2.Character['[Boombox]'].RequiresHandle = false
                        _LocalPlayer2.Character['[Boombox]'].Parent = _LocalPlayer2.Backpack
                    end
                    
                    wait(2.5)
                    
                    -- Hado 90 effect
                    local part = Instance.new('Part')
                    part.Anchored = true
                    part.BottomSurface = Enum.SurfaceType.Smooth
                    part.CanCollide = false
                    part.TopSurface = Enum.SurfaceType.Smooth
                    part.Color = Color3.fromRGB(17, 17, 17)
                    part.Material = Enum.Material.Neon
                    part.Size = Vector3.new(9, 25, 9)
                    
                    local highlight = Instance.new('Highlight')
                    highlight.OutlineColor = Color3.fromRGB(170, 0, 255)
                    highlight.FillColor = Color3.fromRGB(0, 0, 0)
                    highlight.Parent = part
                    part.Parent = workspace
                    
                    part.CFrame = CFrame.new(player.Character.UpperTorso.Position + Vector3.new(0, -50, 0))
                    
                    local tween1 = game:GetService('TweenService'):Create(part, TweenInfo.new(1), {
                        CFrame = CFrame.new(player.Character.UpperTorso.Position + Vector3.new(0, 5, 0))
                    })
                    
                    tween1:Play()
                    tween1.Completed:Wait()
                    
                    -- Send player to void
                    player.Character.UpperTorso.CFrame = CFrame.new(player.Character.UpperTorso.Position + Vector3.new(0, -5000, 0))
                    
                    wait(2)
                    
                    local tween2 = game:GetService('TweenService'):Create(part, TweenInfo.new(1), {
                        CFrame = CFrame.new(player.Character.UpperTorso.Position + Vector3.new(0, 50, 0))
                    })
                    
                    tween2:Play()
                    tween2.Completed:Wait()
                    wait(1)
                    part:Destroy()
                end
            end
        end)

        -- Sex Tool (del GUI con GUI de velocidad)
        if _LocalPlayer2.Backpack:FindFirstChild('Sex') then
            _LocalPlayer2.Backpack:FindFirstChild('Sex'):Destroy()
        end
        task.wait(0.1)
        local sexTool = Instance.new('Tool')
        sexTool.RequiresHandle = false
        sexTool.Name = 'Sex'
        sexTool.Parent = _LocalPlayer2.Backpack
        sexTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                -- Mostrar GUI de velocidad
                createSexSpeedGUI()
                
                getgenv().Furry_ = true
                wait()
                getgenv().Furry_ = false
                
                AnimPlay(4784557631)
                AnimPlay(3119980985, Settings["Grab"]["Sex Speed"] or 10)
                
                while _LocalPlayer2.Character.BodyEffects.Grabbed.Value ~= nil and getgenv().Furry_ == false do
                    task.wait()
                    
                    local v206 = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                    local v207, v208, v209 = pairs(v206:GetDescendants())
                    
                    while true do
                        local v210
                        v209, v210 = v207(v208, v209)
                        if v209 == nil then break end
                        if v210:IsA('BasePart') or v210:IsA('MeshPart') or v210:IsA('Part') then
                            v210.Velocity = Vector3.zero
                            v210.AssemblyAngularVelocity = Vector3.zero
                            v210.AssemblyLinearVelocity = Vector3.zero
                            v210.CanCollide = false
                        end
                    end
                    
                    local v211 = tick() * 133.33
                    Align(10000, 175, 'UpperTorso', 'HumanoidRootPart', CFrame.new(0, -0.3, -2 + 0.5 * math.sin(v211 / 8)), CFrame.Angles(math.pi * -0.3, 0, 0))
                end
                
                Destroy('UpperTorso')
                AnimStopv2(4784557631)
                AnimStopv2(3119980985, 0.5)
            end
        end)

        -- Domain Expansion Tool (del GUI)
        if _LocalPlayer2.Backpack:FindFirstChild('Domain Expansion') then
            _LocalPlayer2.Backpack:FindFirstChild('Domain Expansion'):Destroy()
        end
        task.wait(0.1)
        local domainTool = Instance.new('Tool')
        domainTool.RequiresHandle = false
        domainTool.Name = 'Domain Expansion'
        domainTool.Parent = _LocalPlayer2.Backpack
        domainTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local _Character = _LocalPlayer2.Character
                local _Humanoid = _Character:FindFirstChildOfClass('Humanoid')
                local _BodyEffects = _Character:FindFirstChild('BodyEffects')
                
                if _BodyEffects then
                    _BodyEffects = _Character.BodyEffects:FindFirstChild('Grabbed')
                end
                
                local _FogEnd = game:GetService('Lighting').FogEnd
                local _FogColor = game:GetService('Lighting').FogColor
                local u213 = false
                
                spawn(function()
                    repeat
                        task.wait()
                        Align(8000, 800, 'UpperTorso', 'HumanoidRootPart', CFrame.new(0, 0, -10), CFrame.Angles(0, math.pi, 0))
                    until u213
                end)
                
                if not (_BodyEffects and _BodyEffects.Value) then return end
                if not _BodyEffects.Value:IsA('Model') then return end
                
                -- Domain expansion sound
                if _LocalPlayer2.Backpack:FindFirstChild('[Boombox]') then
                    _LocalPlayer2.Backpack['[Boombox]'].Parent = _LocalPlayer2.Character
                    _ReplicatedStorage.MainEvent:FireServer("Boombox", 7817341182)
                    _LocalPlayer2.Character['[Boombox]'].RequiresHandle = false
                    _LocalPlayer2.Character['[Boombox]'].Parent = _LocalPlayer2.Backpack
                end
                
                wait(1.8)
                
                -- Malevolent Shrine
                if _LocalPlayer2.Backpack:FindFirstChild('[Boombox]') then
                    _LocalPlayer2.Backpack['[Boombox]'].Parent = _LocalPlayer2.Character
                    _ReplicatedStorage.MainEvent:FireServer("Boombox", 7817367014)
                    _LocalPlayer2.Character['[Boombox]'].RequiresHandle = false
                    _LocalPlayer2.Character['[Boombox]'].Parent = _LocalPlayer2.Backpack
                end
                
                wait(5)
                
                -- Domain expansion animation
                local v215 = _Humanoid:LoadAnimation(game:GetObjects('rbxassetid://15554016057')[1])
                local v216, v217, v218 = pairs(_Humanoid:GetPlayingAnimationTracks())
                
                while true do
                    local v219
                    v218, v219 = v216(v217, v218)
                    if v218 == nil then break end
                    if v219.Animation.AnimationId:match('3135389157') or v219.Animation.AnimationId:match('11075367458') then
                        v219:Stop()
                    end
                end
                
                _BodyEffects:GetPropertyChangedSignal('Value'):Connect(function()
                    if not _BodyEffects.Value then
                        game:GetService('TweenService'):Create(game:GetService('Lighting'), TweenInfo.new(1), {
                            FogColor = Color3.fromRGB(100, 87, 72),
                        }):Play()
                        game:GetService('TweenService'):Create(game:GetService('Lighting'), TweenInfo.new(1), {FogEnd = 500}):Play()
                    end
                end)
                
                _Humanoid.WalkSpeed = 0
                wait(2.8)
                v215.Looped = false
                v215:Play()
                wait(2.5)
                
                -- Domain effects
                game:GetService('TweenService'):Create(game:GetService('Lighting'), TweenInfo.new(1), {
                    FogColor = Color3.fromRGB(0, 0, 0),
                }):Play()
                game:GetService('TweenService'):Create(game:GetService('Lighting'), TweenInfo.new(1), {FogEnd = 50}):Play()
                game:GetService('TweenService'):Create(_Humanoid, TweenInfo.new(1), {HipHeight = 10}):Play()
                wait(1)
                v215.Stopped:Wait()
                u213 = true
                
                local v220 = tostring(_LocalPlayer2.Character.BodyEffects.Grabbed.Value)
                if game.Players:FindFirstChild(v220) then
                    game.Players[v220].Character.UpperTorso.CFrame = CFrame.new(0, -120000, 0)
                end
                
                wait(0.3)
                game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing')
                Destroy('UpperTorso')
                _Humanoid.WalkSpeed = 16
                game:GetService('TweenService'):Create(_Humanoid, TweenInfo.new(1), {HipHeight = 2.008}):Play()
                game:GetService('TweenService'):Create(game:GetService('Lighting'), TweenInfo.new(1), {
                    FogColor = _FogColor,
                    FogEnd = _FogEnd,
                }):Play()
            end
        end)

        -- Make Player Stand Tool (del GUI)
