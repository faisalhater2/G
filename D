        local minionTool = Instance.new('Tool')
        minionTool.RequiresHandle = false
        minionTool.Name = 'Minion'
        minionTool.Parent = _LocalPlayer2.Backpack
        minionTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                -- Minion sound
                if _LocalPlayer2.Backpack:FindFirstChild('[Boombox]') then
                    _LocalPlayer2.Backpack['[Boombox]'].Parent = _LocalPlayer2.Character
                    _ReplicatedStorage.MainEvent:FireServer("Boombox", 5986485937)
                    _LocalPlayer2.Character['[Boombox]'].RequiresHandle = false
                    _LocalPlayer2.Character['[Boombox]'].Parent = _LocalPlayer2.Backpack
                end
                
                -- Crear minions alrededor del jugador
                for i = 1, 5 do
                    spawn(function()
                        local minion = Instance.new("Part")
                        minion.Size = Vector3.new(2, 2, 2)
                        minion.Color = Color3.fromRGB(255, 255, 0)
                        minion.Material = Enum.Material.Neon
                        minion.Anchored = true
                        minion.CanCollide = false
                        minion.Parent = workspace
                        
                        local angle = (i / 5) * math.pi * 2
                        local radius = 5
                        
                        local connection
                        connection = game:GetService("RunService").Heartbeat:Connect(function()
                            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                                minion:Destroy()
                                connection:Disconnect()
                                return
                            end
                            
                            local grabbed = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                            if grabbed:FindFirstChild("HumanoidRootPart") then
                                local x = math.cos(angle + tick()) * radius
                                local z = math.sin(angle + tick()) * radius
                                minion.CFrame = grabbed.HumanoidRootPart.CFrame * CFrame.new(x, 0, z)
                            end
                        end)
                    end)
                end
            end
        end)

        -- Make Player Invisible Tool (del GUI)
