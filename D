                spawn(function()
                    repeat
                        task.wait()
                        Align(8000, 800, 'UpperTorso', 'HumanoidRootPart', CFrame.new(0, 0, -2), CFrame.Angles(0, math.pi * 1, 0))
                    until u174
                end)
                wait(1.1)
                
                -- Throw player up
                u174 = true
                spawn(function()
