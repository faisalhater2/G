                        local v223, v224, v225 = pairs(v222:GetDescendants())
                        while true do
                            local v226
                            v225, v226 = v223(v224, v225)
                            if v225 == nil then break end
                            if v226:IsA('BodyPosition') then
                                v226:Destroy()
                            end
                        end
                        standConnection:Disconnect()
                    end
                    
                    for _, partName in pairs({'Head', 'HumanoidRootPart', 'UpperTorso', 'LowerTorso', 'LeftUpperArm', 'RightUpperArm', 'LeftLowerArm', 'RightLowerArm', 'LeftHand', 'RightHand', 'LeftUpperLeg', 'RightUpperLeg', 'LeftLowerLeg', 'RightLowerLeg', 'LeftFoot', 'RightFoot'}) do
                        if u19[partName] and u21[partName] then
                            u27(u19[partName], u21[partName], CFrame.new())
                        end
                    end
                    
                    u21.HumanoidRootPart.CFrame = CFrame.new(rootPart.Position + rootPart.CFrame.LookVector * -3 + rootPart.CFrame.RightVector * -1 + Vector3.new(0, 3.5, 0))
