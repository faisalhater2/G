        -- Mind Control Tool (del GUI con ajustes)
        if _LocalPlayer2.Backpack:FindFirstChild('Mind Control') then
            _LocalPlayer2.Backpack:FindFirstChild('Mind Control'):Destroy()
        end
        task.wait(0.1)
        local mindControlTool = Instance.new('Tool')
        mindControlTool.RequiresHandle = false
        mindControlTool.Name = 'Mind Control'
        mindControlTool.Parent = _LocalPlayer2.Backpack
        
        -- Crear GUI para ajustes de Mind Control
        local function createMindControlGUI()
            local screenGui = Instance.new("ScreenGui", game.CoreGui)
            screenGui.Name = "MindControlGUI"
            
            local frame = Instance.new("Frame", screenGui)
            frame.Size = UDim2.new(0, 250, 0, 200)
            frame.Position = UDim2.new(0.5, -125, 0.5, -100)
            frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
            frame.BorderSizePixel = 0
            
            local title = Instance.new("TextLabel", frame)
            title.Size = UDim2.new(1, 0, 0, 30)
            title.Text = "Mind Control Settings"
            title.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
            title.TextColor3 = Color3.fromRGB(255, 255, 255)
            
            -- Color picker
            local colorLabel = Instance.new("TextLabel", frame)
            colorLabel.Size = UDim2.new(1, 0, 0, 20)
            colorLabel.Position = UDim2.new(0, 0, 0.2, 0)
            colorLabel.Text = "Color:"
            colorLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            colorLabel.BackgroundTransparency = 1
            
            local colorBox = Instance.new("TextBox", frame)
            colorBox.Size = UDim2.new(0.8, 0, 0, 25)
            colorBox.Position = UDim2.new(0.1, 0, 0.3, 0)
            colorBox.Text = "255,255,255"
            colorBox.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
            colorBox.TextColor3 = Color3.fromRGB(255, 255, 255)
            
            -- Thickness slider
            local thicknessLabel = Instance.new("TextLabel", frame)
            thicknessLabel.Size = UDim2.new(1, 0, 0, 20)
            thicknessLabel.Position = UDim2.new(0, 0, 0.45, 0)
            thicknessLabel.Text = "Thickness: " .. Settings["Grab"]["Mind Control Thickness"]
            thicknessLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            thicknessLabel.BackgroundTransparency = 1
            
            local thicknessSlider = Instance.new("TextBox", frame)
            thicknessSlider.Size = UDim2.new(0.8, 0, 0, 25)
            thicknessSlider.Position = UDim2.new(0.1, 0, 0.55, 0)
            thicknessSlider.Text = tostring(Settings["Grab"]["Mind Control Thickness"])
            thicknessSlider.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
            thicknessSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
            
            -- Frequency slider
            local freqLabel = Instance.new("TextLabel", frame)
            freqLabel.Size = UDim2.new(1, 0, 0, 20)
            freqLabel.Position = UDim2.new(0, 0, 0.7, 0)
            freqLabel.Text = "Frequency: " .. Settings["Grab"]["Mind Control Frequency"]
            freqLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            freqLabel.BackgroundTransparency = 1
            
            local freqSlider = Instance.new("TextBox", frame)
            freqSlider.Size = UDim2.new(0.8, 0, 0, 25)
            freqSlider.Position = UDim2.new(0.1, 0, 0.8, 0)
            freqSlider.Text = tostring(Settings["Grab"]["Mind Control Frequency"])
            freqSlider.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
            freqSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
            
            local button = Instance.new("TextButton", frame)
            button.Size = UDim2.new(0.6, 0, 0, 30)
            button.Position = UDim2.new(0.2, 0, 0.9, 0)
            button.Text = "Apply Settings"
            button.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
            button.TextColor3 = Color3.fromRGB(255, 255, 255)
            
            button.MouseButton1Click:Connect(function()
                -- Parse color
                local colorParts = {}
                for part in string.gmatch(colorBox.Text, "[^,]+") do
                    table.insert(colorParts, tonumber(part))
                end
                if #colorParts == 3 then
                    Settings["Grab"]["Mind Control Color"] = Color3.fromRGB(colorParts[1], colorParts[2], colorParts[3])
                end
                
                -- Parse thickness
                local thickness = tonumber(thicknessSlider.Text)
                if thickness and thickness >= 0.1 and thickness <= 10 then
                    Settings["Grab"]["Mind Control Thickness"] = thickness
                end
                
                -- Parse frequency
                local frequency = tonumber(freqSlider.Text)
                if frequency and frequency >= 0.1 and frequency <= 15 then
                    Settings["Grab"]["Mind Control Frequency"] = frequency
                end
                
                screenGui:Destroy()
            end)
            
            -- Ocultar GUI cuando se suelte el ragdoll
            local connection
            connection = game:GetService("RunService").Heartbeat:Connect(function()
                if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild("BodyEffects") or 
                   not _LocalPlayer2.Character.BodyEffects:FindFirstChild("Grabbed") or 
                   _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                    screenGui:Destroy()
                    connection:Disconnect()
                end
            end)
        end
        
        mindControlTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                -- Mostrar GUI de ajustes
                createMindControlGUI()
                
                -- Crear efecto de control mental
                local beam = Instance.new("Beam")
                beam.Color = ColorSequence.new(Settings["Grab"]["Mind Control Color"])
                beam.Width0 = Settings["Grab"]["Mind Control Thickness"]
                beam.Width1 = Settings["Grab"]["Mind Control Thickness"]
                beam.FaceCamera = true
                
                local attachment0 = Instance.new("Attachment")
                attachment0.Parent = _LocalPlayer2.Character.Head
                local attachment1 = Instance.new("Attachment")
                attachment1.Parent = _LocalPlayer2.Character.BodyEffects.Grabbed.Value.Head
                
                beam.Attachment0 = attachment0
                beam.Attachment1 = attachment1
                beam.Parent = workspace
                
                -- Hacer que el jugador agarrado siga al jugador
                local controlConnection
                controlConnection = game:GetService("RunService").Heartbeat:Connect(function()
                    if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                        beam:Destroy()
                        attachment0:Destroy()
                        attachment1:Destroy()
                        controlConnection:Disconnect()
                        return
                    end
                    
                    -- Mover al jugador agarrado hacia el jugador
                    local grabbed = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                    if grabbed:FindFirstChild("HumanoidRootPart") then
                        local direction = (_LocalPlayer2.Character.HumanoidRootPart.Position - grabbed.HumanoidRootPart.Position).Unit
                        grabbed.HumanoidRootPart.Velocity = direction * Settings["Grab"]["Mind Control Frequency"] * 10
                    end
                end)
                
                -- Destruir beam cuando se suelte
                _LocalPlayer2.Character.BodyEffects.Grabbed:GetPropertyChangedSignal("Value"):Connect(function()
                    if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                        beam:Destroy()
                        attachment0:Destroy()
                        attachment1:Destroy()
                        if controlConnection then
                            controlConnection:Disconnect()
                        end
                    end
                end)
            end
        end)

        -- Hoverboard Tool (del GUI)
        if _LocalPlayer2.Backpack:FindFirstChild('Hoverboard') then
            _LocalPlayer2.Backpack:FindFirstChild('Hoverboard'):Destroy()
        end
        task.wait(0.1)
        local hoverboardTool = Instance.new('Tool')
        hoverboardTool.RequiresHandle = false
        hoverboardTool.Name = 'Hoverboard'
        hoverboardTool.Parent = _LocalPlayer2.Backpack
        hoverboardTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                -- Crear hoverboard effect
                local hoverPart = Instance.new("Part")
                hoverPart.Size = Vector3.new(4, 0.5, 8)
                hoverPart.Color = Color3.fromRGB(0, 170, 255)
                hoverPart.Material = Enum.Material.Neon
                hoverPart.Transparency = 0.3
                hoverPart.Anchored = true
                hoverPart.CanCollide = false
                hoverPart.Parent = workspace
                
                local hoverConnection
                hoverConnection = game:GetService("RunService").Heartbeat:Connect(function()
                    if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                        hoverPart:Destroy()
                        hoverConnection:Disconnect()
                        return
                    end
                    
                    local grabbed = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                    if grabbed:FindFirstChild("HumanoidRootPart") then
                        hoverPart.CFrame = grabbed.HumanoidRootPart.CFrame * CFrame.new(0, -3, 0)
                        
                        -- Hacer flotar al jugador
                        grabbed.HumanoidRootPart.Velocity = Vector3.new(grabbed.HumanoidRootPart.Velocity.X, 5, grabbed.HumanoidRootPart.Velocity.Z)
                    end
                end)
                
                -- Destruir hoverboard cuando se suelte
                _LocalPlayer2.Character.BodyEffects.Grabbed:GetPropertyChangedSignal("Value"):Connect(function()
                    if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                        hoverPart:Destroy()
                        if hoverConnection then
                            hoverConnection:Disconnect()
                        end
                    end
                end)
            end
        end)

        -- Minion Tool (del GUI - versiÃ³n simplificada)
        if _LocalPlayer2.Backpack:FindFirstChild('Minion') then
            _LocalPlayer2.Backpack:FindFirstChild('Minion'):Destroy()
        end
