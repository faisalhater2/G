                for _, part in pairs(grabbed:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.Transparency = 1
                    elseif part:IsA("Decal") then
                        part.Transparency = 1
                    end
                end
                
                -- Restaurar visibilidad cuando se suelte
                local connection
                connection = _LocalPlayer2.Character.BodyEffects.Grabbed:GetPropertyChangedSignal("Value"):Connect(function()
                    if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                        for _, part in pairs(grabbed:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.Transparency = 0
                            elseif part:IsA("Decal") then
                                part.Transparency = 0
                            end
                        end
                        connection:Disconnect()
                    end
                end)
            end
        end)

