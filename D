            ["Mind Control Color"] = Color3.fromRGB(255,255,255),
            ["Mind Control Thickness"] = 1,
            ["Mind Control Frequency"] = 1,
        }
    }

    -- FunciÃ³n para crear una GUI de ajustes de velocidad para Sex
    local function createSexSpeedGUI()
        local screenGui = Instance.new("ScreenGui", game.CoreGui)
        screenGui.Name = "SexSpeedGUI"
        
        local frame = Instance.new("Frame", screenGui)
        frame.Size = UDim2.new(0, 200, 0, 100)
        frame.Position = UDim2.new(0.5, -100, 0.5, -50)
        frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
        frame.BorderSizePixel = 0
        
        local title = Instance.new("TextLabel", frame)
        title.Size = UDim2.new(1, 0, 0, 30)
        title.Text = "Sex Speed Control"
        title.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        title.TextColor3 = Color3.fromRGB(255, 255, 255)
        
        local slider = Instance.new("TextBox", frame)
        slider.Size = UDim2.new(0.8, 0, 0, 30)
        slider.Position = UDim2.new(0.1, 0, 0.4, 0)
        slider.Text = tostring(Settings["Grab"]["Sex Speed"])
        slider.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        slider.TextColor3 = Color3.fromRGB(255, 255, 255)
        
        local button = Instance.new("TextButton", frame)
        button.Size = UDim2.new(0.6, 0, 0, 30)
        button.Position = UDim2.new(0.2, 0, 0.7, 0)
        button.Text = "Apply Speed"
        button.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        
        button.MouseButton1Click:Connect(function()
            local speed = tonumber(slider.Text)
            if speed and speed >= 1 and speed <= 50 then
                Settings["Grab"]["Sex Speed"] = speed
                screenGui:Destroy()
            end
        end)
        
        -- Ocultar GUI cuando se suelte el ragdoll
        local connection
        connection = game:GetService("RunService").Heartbeat:Connect(function()
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild("BodyEffects") or 
               not _LocalPlayer2.Character.BodyEffects:FindFirstChild("Grabbed") or 
               _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                screenGui:Destroy()
                connection:Disconnect()
            end
        end)
    end

    function ToolSSs()
        if _LocalPlayer2.Backpack:FindFirstChild('Activate') then
            _LocalPlayer2.Backpack:FindFirstChild('Activate'):Destroy()
        end

        task.wait(0.1)

        local _Tool = Instance.new('Tool')

        _Tool.RequiresHandle = false
        _Tool.Name = 'Activate'
        _Tool.Parent = _LocalPlayer2.Backpack

        _Tool.Activated:Connect(function()
            game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', true)
            wait(0.3)
            Grab()
        end)

        -- Time Stop Tool (del GUI)
        if _LocalPlayer2.Backpack:FindFirstChild('Time Stop') then
            _LocalPlayer2.Backpack:FindFirstChild('Time Stop'):Destroy()
        end
        task.wait(0.1)
        local timeStopTool = Instance.new('Tool')
        timeStopTool.RequiresHandle = false
        timeStopTool.Name = 'Time Stop'
        timeStopTool.Parent = _LocalPlayer2.Backpack
        timeStopTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local v162, v163, v164 = pairs(game:GetService('Players').LocalPlayer.Character:WaitForChild('Humanoid'):GetPlayingAnimationTracks())
                while true do
                    local v165
                    v164, v165 = v162(v163, v164)
                    if v164 == nil then break end
                    if v165.Animation.AnimationId == 'rbxassetid://3135389157' then
                        v165:Stop()
                    end
                end
                
                local u166 = false
                spawn(function()
                    repeat
                        task.wait()
                        Align(8000, 800, 'UpperTorso', 'HumanoidRootPart', CFrame.new(0, 0, -10), CFrame.Angles(0, math.pi * 1, 0))
                    until u166
                end)
                
                -- Play time stop sound
                if _LocalPlayer2.Backpack:FindFirstChild('[Boombox]') then
                    _LocalPlayer2.Backpack['[Boombox]'].Parent = _LocalPlayer2.Character
                    _ReplicatedStorage.MainEvent:FireServer("Boombox", 5455437798)
                    _LocalPlayer2.Character['[Boombox]'].RequiresHandle = false
                    _LocalPlayer2.Character['[Boombox]'].Parent = _LocalPlayer2.Backpack
                end
                
                wait(2.5)
                
                -- Time stop animation
                AnimPlay(10714177846, 1)
                wait(3)
                
                -- Ora barrage
                for i = 1,10 do
                    AnimStopv2(8254794168)
                    AnimPlay(8254787838, 9)
                    wait(0.2)
                    AnimStopv2(8254787838)
                    AnimPlay(8254794168, 9)
                    wait(0.2)
                end
                
                -- Final punch
                AnimPlay(3354696735, 1.5)
                wait(1)
                
                -- Throw player
                local v167 = tostring(_LocalPlayer2.Character.BodyEffects.Grabbed.Value)
                if game.Players:FindFirstChild(v167) then
                    local _Character = game.Players[v167].Character
                    if _Character and _Character:FindFirstChild('UpperTorso') then
                        _Character.UpperTorso.CFrame = CFrame.new(0, -1200, 0)
                    end
                end
                
                wait(0.3)
                _ReplicatedStorage.MainEvent:FireServer('Grabbing')
                u166 = true
            end
        end)

        -- Get Over Here Tool (del GUI)
        if _LocalPlayer2.Backpack:FindFirstChild('Get Over Here') then
            _LocalPlayer2.Backpack:FindFirstChild('Get Over Here'):Destroy()
        end
        task.wait(0.1)
        local getOverHereTool = Instance.new('Tool')
        getOverHereTool.RequiresHandle = false
        getOverHereTool.Name = 'Get Over Here'
        getOverHereTool.Parent = _LocalPlayer2.Backpack
        getOverHereTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local v170, v171, v172 = pairs(game:GetService('Players').LocalPlayer.Character:WaitForChild('Humanoid'):GetPlayingAnimationTracks())
                while true do
                    local v173
                    v172, v173 = v170(v171, v172)
                    if v172 == nil then break end
                    if v173.Animation.AnimationId == 'rbxassetid://3135389157' then
                        v173:Stop()
                    end
                end
                
                -- Play chain sound
                if _LocalPlayer2.Backpack:FindFirstChild('[Boombox]') then
                    _LocalPlayer2.Backpack['[Boombox]'].Parent = _LocalPlayer2.Character
                    _ReplicatedStorage.MainEvent:FireServer("Boombox", 8643750815)
                    _LocalPlayer2.Character['[Boombox]'].RequiresHandle = false
                    _LocalPlayer2.Character['[Boombox]'].Parent = _LocalPlayer2.Backpack
                end
                
                wait(1.6)
                
                -- Get over here animation
                AnimPlay(16768625968, 1)
                
                -- Pull player close
