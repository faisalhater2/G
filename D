        -- Control Tool (original de Zeraya - MANTENER)
        if _LocalPlayer2.Backpack:FindFirstChild('Control') then
            _LocalPlayer2.Backpack:FindFirstChild('Control'):Destroy()
        end

        task.wait(0.1)

        local _Tool24 = Instance.new('Tool', _LocalPlayer2.Backpack)

        _Tool24.RequiresHandle = false
        _Tool24.Name = 'Control'

        _Tool24.Activated:connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                task.wait(1)

                _LocalPlayer2.Character.Control.Parent = _LocalPlayer2.Backpack

                task.wait()

                u20 = true

                task.wait()

                u20 = false
                u21 = u107(u19)
                workspace.CurrentCamera.CameraSubject = u21

                u31(u22, u37('Heartbeat', function()
                    local v525, v526, v527 = pairs(_LocalPlayer2.Character:FindFirstChild('Humanoid'):GetPlayingAnimationTracks())

                    while true do
                        local v528

                        v527, v528 = v525(v526, v527)

                        if v527 == nil then
                            break
                        end

                        v528:Stop()
                    end

                    _LocalPlayer2.Character.HumanoidRootPart.CFrame = u21.HumanoidRootPart.CFrame * CFrame.new(0, -15, 0) * CFrame.Angles(math.pi * 0.5, 0, 0)

                    ZeroVelocity___(_LocalPlayer2.Character.HumanoidRootPart)

                    local v529 = u21

                    u27(u19.Head, v529:FindFirstChild('Head'), CFrame.new(0, 0, 0))

                    local v530 = u21

                    u27(u19.HumanoidRootPart, v530:FindFirstChild('HumanoidRootPart'), CFrame.new(0, 0, 0))

                    local v531 = u21

                    u27(u19.UpperTorso, v531:FindFirstChild('UpperTorso'), CFrame.new(0, 0, 0))

                    local v532 = u21

                    u27(u19.LowerTorso, v532:FindFirstChild('LowerTorso'), CFrame.new(0, 0, 0))

                    local v533 = u21

                    u27(u19.LeftUpperArm, v533:FindFirstChild('LeftUpperArm'), CFrame.new(0, 0, 0))

                    local v534 = u21

                    u27(u19.LeftLowerArm, v534:FindFirstChild('LeftLowerArm'), CFrame.new(0, 0, 0))

                    local v535 = u21

                    u27(u19.RightUpperArm, v535:FindFirstChild('RightUpperArm'), CFrame.new(0, 0, 0))

                    local v536 = u21

