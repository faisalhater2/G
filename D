                        task.wait()
                        Align(8000, 800, 'UpperTorso', 'HumanoidRootPart', CFrame.new(0, 40, 0), CFrame.Angles(0, math.pi * 1, 0))
                    until true
                end)
                wait(0.9)
                
                local v175 = tostring(_LocalPlayer2.Character.BodyEffects.Grabbed.Value)
                if game.Players:FindFirstChild(v175) then
                    local _Character = game.Players[v175].Character
                    if _Character and _Character:FindFirstChild('UpperTorso') then
                        _Character.UpperTorso.CFrame = CFrame.new(0, -12000, 0)
