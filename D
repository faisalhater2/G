        -- Orbit Tool (del GUI)
        if _LocalPlayer2.Backpack:FindFirstChild('Orbit') then
            _LocalPlayer2.Backpack:FindFirstChild('Orbit'):Destroy()
        end
        task.wait(0.1)
        local orbitTool = Instance.new('Tool')
        orbitTool.RequiresHandle = false
        orbitTool.Name = 'Orbit'
        orbitTool.Parent = _LocalPlayer2.Backpack
        local orbitConnection = nil
        orbitTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                if orbitConnection then
                    orbitConnection:Disconnect()
                    orbitConnection = nil
                end
                
                local currentTime = 0
                orbitConnection = game:GetService('RunService').Heartbeat:Connect(function(delta)
                    if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                        orbitConnection:Disconnect()
                        orbitConnection = nil
                        return
                    end
                    
                    currentTime = currentTime + delta
                    local player = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                    
                    if player:FindFirstChild('UpperTorso') then
                        player.UpperTorso.CFrame = CFrame.Angles(math.pi * 0.1, 1.5 * math.pi * currentTime, 0) * CFrame.new(1, 2, 5) + _LocalPlayer2.Character.HumanoidRootPart.Position
                    end
                    if player:FindFirstChild('LowerTorso') then
                        player.LowerTorso.CFrame = CFrame.Angles(math.pi * -0.1, 1.5 * math.pi * -currentTime, 0) * CFrame.new(1, 2, 5) + _LocalPlayer2.Character.HumanoidRootPart.Position
                    end
                end)
            end
        end)

        -- Made In Heaven Tool (del GUI)
        if _LocalPlayer2.Backpack:FindFirstChild('Made In Heaven') then
            _LocalPlayer2.Backpack:FindFirstChild('Made In Heaven'):Destroy()
        end
        task.wait(0.1)
        local madeInHeavenTool = Instance.new('Tool')
        madeInHeavenTool.RequiresHandle = false
        madeInHeavenTool.Name = 'Made In Heaven'
        madeInHeavenTool.Parent = _LocalPlayer2.Backpack
        madeInHeavenTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local v194, v195, v196 = pairs(game:GetService('Players').LocalPlayer.Character:WaitForChild('Humanoid'):GetPlayingAnimationTracks())
                while true do
                    local v197
                    v196, v197 = v194(v195, v196)
                    if v196 == nil then break end
                    if v197.Animation.AnimationId == 'rbxassetid://3135389157' then
                        v197:Stop()
                    end
                end
                
                -- Made in Heaven sound
                if _LocalPlayer2.Backpack:FindFirstChild('[Boombox]') then
                    _LocalPlayer2.Backpack['[Boombox]'].Parent = _LocalPlayer2.Character
                    _ReplicatedStorage.MainEvent:FireServer("Boombox", 8004825017)
                    _LocalPlayer2.Character['[Boombox]'].RequiresHandle = false
                    _LocalPlayer2.Character['[Boombox]'].Parent = _LocalPlayer2.Backpack
                end
                
                _LocalPlayer2.Character.Humanoid.WalkSpeed = 0
                
                -- Made in Heaven animation
                AnimPlay(11394033602, 1)
                
                -- Heaven effects
                game:GetService('TweenService'):Create(workspace.CurrentCamera, TweenInfo.new(1), {FieldOfView = 120}):Play()
                game:GetService('TweenService'):Create(game.Lighting, TweenInfo.new(1), {FogEnd = 20, FogColor = Color3.new(1, 1, 1)}):Play()
                game:GetService('TweenService'):Create(_LocalPlayer2.Character.Humanoid, TweenInfo.new(1), {HipHeight = 3.5}):Play()
                
                wait(3)
                
                -- Throw player to heaven
                local u198 = false
                spawn(function()
                    repeat
                        task.wait()
                        Align(8000, 800, 'UpperTorso', 'HumanoidRootPart', CFrame.new(0, 50, -20), CFrame.Angles(0, math.pi * 1, 0))
                    until u198
                end)
                wait(7)
                
                local v199 = tostring(_LocalPlayer2.Character.BodyEffects.Grabbed.Value)
                if game.Players:FindFirstChild(v199) then
                    game.Players[v199].Character.UpperTorso.CFrame = CFrame.new(0, -12000, 0)
                end
                
                wait(0.9)
                _ReplicatedStorage.MainEvent:FireServer('Grabbing')
                u198 = true
                
                -- Reset effects
                AnimStopv2(11394033602, 0.5)
                game:GetService('TweenService'):Create(workspace.CurrentCamera, TweenInfo.new(1), {FieldOfView = 70}):Play()
                game:GetService('TweenService'):Create(game.Lighting, TweenInfo.new(1), {FogEnd = 500, FogColor = Color3.new(1, 1, 1)}):Play()
                game:GetService('TweenService'):Create(_LocalPlayer2.Character.Humanoid, TweenInfo.new(1), {HipHeight = 2.008}):Play()
                _LocalPlayer2.Character.Humanoid.WalkSpeed = 16
            end
        end)

        -- C Moon Tool (del GUI)
        if _LocalPlayer2.Backpack:FindFirstChild('C Moon') then
            _LocalPlayer2.Backpack:FindFirstChild('C Moon'):Destroy()
        end
        task.wait(0.1)
        local cMoonTool = Instance.new('Tool')
        cMoonTool.RequiresHandle = false
        cMoonTool.Name = 'C Moon'
        cMoonTool.Parent = _LocalPlayer2.Backpack
        cMoonTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                _LocalPlayer2.Character.Humanoid.WalkSpeed = 0
                
                -- C Moon sound
                if _LocalPlayer2.Backpack:FindFirstChild('[Boombox]') then
                    _LocalPlayer2.Backpack['[Boombox]'].Parent = _LocalPlayer2.Character
                    _ReplicatedStorage.MainEvent:FireServer("Boombox", 8063279487)
                    _LocalPlayer2.Character['[Boombox]'].RequiresHandle = false
                    _LocalPlayer2.Character['[Boombox]'].Parent = _LocalPlayer2.Backpack
                end
                
