                game:GetService('TweenService'):Create(game.Lighting, TweenInfo.new(0.7), {ColorShift_Bottom = Color3.fromRGB(0,255,0)}):Play()
                game:GetService('TweenService'):Create(game.Lighting, TweenInfo.new(0.7), {ColorShift_Top = Color3.fromRGB(0,255,0)}):Play()
                game:GetService('TweenService'):Create(game.Lighting, TweenInfo.new(0.7), {Brightness = 100}):Play()
                game:GetService('TweenService'):Create(game.Lighting, TweenInfo.new(0.7), {Ambient = Color3.fromRGB(0,255,0)}):Play()
                
