    local function u107(p101)
        p101.Archivable = true

        local v102 = p101:Clone()

        v102.HumanoidRootPart.Anchored = false
        v102.Humanoid.MaxHealth = 9000000000
        v102.Humanoid.Health = 9000000000

        v102.RagdollConstraints:Destroy()
        v102.BodyEffects:Destroy()
        v102:WaitForChild('GRABBING_CONSTRAINT'):Destroy()

        local v103, v104, v105 = pairs(v102:GetDescendants())

        while true do
            local v106

            v105, v106 = v103(v104, v105)

            if v105 == nil then
                break
            end
            if v106:IsA('BasePart') and (v106.Name ~= 'Head' and (v106.Name ~= 'HumanoidRootPart' and (v106.Name ~= 'UpperTorso' and (v106.Name ~= 'LowerTorso' and (v106.Name ~= 'LeftUpperArm' and (v106.Name ~= 'RightUpperArm' and (v106.Name ~= 'LeftLowerArm' and (v106.Name ~= 'RightLowerArm' and (v106.Name ~= 'LeftHand' and (v106.Name ~= 'RightHand' and (v106.Name ~= 'LeftUpperLeg' and (v106.Name ~= 'RightUpperLeg' and (v106.Name ~= 'LeftLowerLeg' and (v106.Name ~= 'RightLowerLeg' and (v106.Name ~= 'LeftFoot' and v106.Name ~= 'RightFoot'))))))))))))))) then
                v106.Massless = false

                v106:Destroy()
            end
            if v106:IsA('BasePart') or (v106:IsA('MeshPart') or v106:IsA('Part')) then
                v106.CustomPhysicalProperties = PhysicalProperties.new(100, 2, 0.5, 100, 1)
                v106.Transparency = 1
                v106.CanCollide = false

                if v106:FindFirstChildWhichIsA('BodyGyro') and v106:FindFirstChildWhichIsA('BodyPosition') then
                    v106:Destroy()
                end
            end
            if v106:IsA('Decal') then
                v106.Transparency = 1
            end
            if v106:IsA('ForceField') then
                v106:Destroy()
            end
            if v106:IsA('Motor6D') then
                v106:Destroy()
            end
        end

        v102.HumanoidRootPart.Transparency = 1
        v102.Parent = _LocalPlayer2.Character

        v102.Humanoid:ChangeState('GettingUp')

        v102.HumanoidRootPart.CFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame
        p101.Archivable = false
        p101.LeftHand:FindFirstChildOfClass('Motor6D').Enabled = false
        p101.RightHand:FindFirstChildOfClass('Motor6D').Enabled = false
        p101.LeftFoot:FindFirstChildOfClass('Motor6D').Enabled = false
        p101.RightFoot:FindFirstChildOfClass('Motor6D').Enabled = false

        return v102
    end

    function AnimStop(p108, p109)
        local v110, v111, v112 = pairs(game.Players.LocalPlayer.Character:WaitForChild('Humanoid'):GetPlayingAnimationTracks())

        while true do
            local v113

            v112, v113 = v110(v111, v112)

            if v112 == nil then
                break
            end
            if v113.Animation.AnimationId:match('rbxassetid://' .. p108) then
                if tonumber(p109) then
                    v113:Stop(p109)
                else
                    v113:Stop()
                end
            end
        end
    end

    -- ===== FUNCIÓN PARA CREAR UI DE VELOCIDAD DEL SEX =====
    function CreateSexSpeedUI()
        if SexSpeedUI then
            SexSpeedUI:Destroy()
            SexSpeedUI = nil
            return
        end
        
        SexSpeedUI = Instance.new("ScreenGui")
        SexSpeedUI.Name = "SexSpeedUI"
        SexSpeedUI.Parent = game.CoreGui
        
        local Frame = Instance.new("Frame")
        Frame.Size = UDim2.new(0, 200, 0, 100)
        Frame.Position = UDim2.new(0.5, -100, 0.5, -50)
        Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        Frame.BorderSizePixel = 0
        Frame.Parent = SexSpeedUI
        
        local UICorner = Instance.new("UICorner")
        UICorner.CornerRadius = UDim.new(0, 8)
        UICorner.Parent = Frame
        
        local Title = Instance.new("TextLabel")
        Title.Text = "Sex Speed"
        Title.Size = UDim2.new(1, 0, 0, 30)
        Title.BackgroundTransparency = 1
        Title.TextColor3 = Color3.fromRGB(255, 255, 255)
        Title.Font = Enum.Font.SourceSansBold
        Title.TextSize = 18
        Title.Parent = Frame
        
        local Slider = Instance.new("Frame")
        Slider.Size = UDim2.new(0.8, 0, 0, 30)
        Slider.Position = UDim2.new(0.1, 0, 0.4, 0)
        Slider.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        Slider.Parent = Frame
        
        local SliderUICorner = Instance.new("UICorner")
        SliderUICorner.Parent = Slider
        
        local Fill = Instance.new("Frame")
        Fill.Size = UDim2.new((Settings["Grab"]["Sex Speed"] or 10) / 20, 0, 1, 0)
        Fill.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
        Fill.Parent = Slider
        
        local FillUICorner = Instance.new("UICorner")
        FillUICorner.Parent = Fill
        
        local Value = Instance.new("TextLabel")
        Value.Text = tostring(Settings["Grab"]["Sex Speed"] or 10)
        Value.Size = UDim2.new(1, 0, 0, 20)
        Value.Position = UDim2.new(0, 0, 0.7, 0)
        Value.BackgroundTransparency = 1
        Value.TextColor3 = Color3.fromRGB(255, 255, 255)
        Value.Font = Enum.Font.SourceSans
        Value.TextSize = 16
        Value.Parent = Frame
        
        local SliderButton = Instance.new("TextButton")
        SliderButton.Size = UDim2.new(1, 0, 1, 0)
        SliderButton.BackgroundTransparency = 1
        SliderButton.Text = ""
        SliderButton.Parent = Slider
        
        SliderButton.MouseButton1Down:Connect(function()
            local Connection
            Connection = _RunService.RenderStepped:Connect(function()
                local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
                local Percent = (Mouse.X - Slider.AbsolutePosition.X) / Slider.AbsoluteSize.X
                Percent = math.clamp(Percent, 0, 1)
                
                Fill.Size = UDim2.new(Percent, 0, 1, 0)
                local Speed = math.floor(Percent * 20)
                Settings["Grab"]["Sex Speed"] = Speed
                Value.Text = tostring(Speed)
            end)
            
            local Release
            Release = game:GetService("UserInputService").InputEnded:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 then
                    Connection:Disconnect()
                    Release:Disconnect()
                end
            end)
        end)
        
        -- Cerrar cuando el jugador se quite el ragdoll
        if u19 then
            u19:GetPropertyChangedSignal("Parent"):Connect(function()
                if SexSpeedUI then
                    SexSpeedUI:Destroy()
                    SexSpeedUI = nil
                end
            end)
        end
    end

    -- ===== FUNCIÓN PARA CREAR UI DE MIND CONTROL =====
    function CreateMindControlUI()
        if MindControlUI then
            MindControlUI:Destroy()
            MindControlUI = nil
            return
        end
        
        MindControlUI = Instance.new("ScreenGui")
        MindControlUI.Name = "MindControlUI"
        MindControlUI.Parent = game.CoreGui
        
        local Frame = Instance.new("Frame")
        Frame.Size = UDim2.new(0, 250, 0, 150)
        Frame.Position = UDim2.new(0.5, -125, 0.5, -75)
        Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        Frame.BorderSizePixel = 0
        Frame.Parent = MindControlUI
        
        local UICorner = Instance.new("UICorner")
        UICorner.CornerRadius = UDim.new(0, 8)
        UICorner.Parent = Frame
        
        local Title = Instance.new("TextLabel")
        Title.Text = "Mind Control Settings"
        Title.Size = UDim2.new(1, 0, 0, 30)
        Title.BackgroundTransparency = 1
        Title.TextColor3 = Color3.fromRGB(255, 255, 255)
        Title.Font = Enum.Font.SourceSansBold
        Title.TextSize = 18
        Title.Parent = Frame
        
        -- Color Picker
        local ColorLabel = Instance.new("TextLabel")
        ColorLabel.Text = "Color"
        ColorLabel.Size = UDim2.new(0.4, 0, 0, 20)
        ColorLabel.Position = UDim2.new(0.05, 0, 0.25, 0)
        ColorLabel.BackgroundTransparency = 1
        ColorLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        ColorLabel.Font = Enum.Font.SourceSans
        ColorLabel.TextSize = 14
        ColorLabel.Parent = Frame
        
        local ColorBox = Instance.new("Frame")
        ColorBox.Size = UDim2.new(0.2, 0, 0, 20)
        ColorBox.Position = UDim2.new(0.5, 0, 0.25, 0)
        ColorBox.BackgroundColor3 = Settings["Grab"]["Mind Control Color"]
        ColorBox.BorderSizePixel = 1
        ColorBox.BorderColor3 = Color3.fromRGB(255, 255, 255)
        ColorBox.Parent = Frame
        
        ColorBox.MouseButton1Click:Connect(function()
            local ColorPicker = Instance.new("ScreenGui")
            ColorPicker.Name = "ColorPicker"
            ColorPicker.Parent = game.CoreGui
            
            -- Simple color picker (puedes mejorarlo)
            local Colors = {
                Color3.fromRGB(255, 255, 255),
                Color3.fromRGB(255, 0, 0),
                Color3.fromRGB(0, 255, 0),
                Color3.fromRGB(0, 0, 255),
                Color3.fromRGB(255, 255, 0),
                Color3.fromRGB(255, 0, 255),
                Color3.fromRGB(0, 255, 255)
            }
            
            for i, color in ipairs(Colors) do
                local ColorButton = Instance.new("Frame")
                ColorButton.Size = UDim2.new(0, 30, 0, 30)
                ColorButton.Position = UDim2.new(0, 10 + (i-1)*35, 0, 10)
                ColorButton.BackgroundColor3 = color
                ColorButton.BorderSizePixel = 1
                ColorButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
                ColorButton.Parent = ColorPicker
                
                ColorButton.MouseButton1Click:Connect(function()
                    Settings["Grab"]["Mind Control Color"] = color
                    ColorBox.BackgroundColor3 = color
                    ColorPicker:Destroy()
                end)
            end
        end)
        
        -- Thickness Slider
        local ThicknessLabel = Instance.new("TextLabel")
        ThicknessLabel.Text = "Thickness: " .. Settings["Grab"]["Mind Control Thickness"]
        ThicknessLabel.Size = UDim2.new(0.9, 0, 0, 20)
        ThicknessLabel.Position = UDim2.new(0.05, 0, 0.45, 0)
        ThicknessLabel.BackgroundTransparency = 1
        ThicknessLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        ThicknessLabel.Font = Enum.Font.SourceSans
        ThicknessLabel.TextSize = 14
        ThicknessLabel.Parent = Frame
        
        local ThicknessSlider = Instance.new("Frame")
        ThicknessSlider.Size = UDim2.new(0.9, 0, 0, 10)
        ThicknessSlider.Position = UDim2.new(0.05, 0, 0.55, 0)
        ThicknessSlider.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        ThicknessSlider.Parent = Frame
        
        local ThicknessFill = Instance.new("Frame")
        ThicknessFill.Size = UDim2.new(Settings["Grab"]["Mind Control Thickness"] / 10, 0, 1, 0)
        ThicknessFill.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
        ThicknessFill.Parent = ThicknessSlider
        
        local ThicknessButton = Instance.new("TextButton")
        ThicknessButton.Size = UDim2.new(1, 0, 1, 0)
        ThicknessButton.BackgroundTransparency = 1
        ThicknessButton.Text = ""
        ThicknessButton.Parent = ThicknessSlider
        
        ThicknessButton.MouseButton1Down:Connect(function()
            local Connection
            Connection = _RunService.RenderStepped:Connect(function()
                local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
                local Percent = (Mouse.X - ThicknessSlider.AbsolutePosition.X) / ThicknessSlider.AbsoluteSize.X
                Percent = math.clamp(Percent, 0, 1)
                
                ThicknessFill.Size = UDim2.new(Percent, 0, 1, 0)
                local Thickness = math.floor(Percent * 10)
                Settings["Grab"]["Mind Control Thickness"] = Thickness
                ThicknessLabel.Text = "Thickness: " .. Thickness
            end)
            
            local Release
            Release = game:GetService("UserInputService").InputEnded:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 then
                    Connection:Disconnect()
                    Release:Disconnect()
                end
            end)
        end)
        
        -- Frequency Slider
        local FrequencyLabel = Instance.new("TextLabel")
        FrequencyLabel.Text = "Frequency: " .. Settings["Grab"]["Mind Control Frequency"]
        FrequencyLabel.Size = UDim2.new(0.9, 0, 0, 20)
        FrequencyLabel.Position = UDim2.new(0.05, 0, 0.65, 0)
        FrequencyLabel.BackgroundTransparency = 1
        FrequencyLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        FrequencyLabel.Font = Enum.Font.SourceSans
        FrequencyLabel.TextSize = 14
        FrequencyLabel.Parent = Frame
        
        local FrequencySlider = Instance.new("Frame")
        FrequencySlider.Size = UDim2.new(0.9, 0, 0, 10)
        FrequencySlider.Position = UDim2.new(0.05, 0, 0.75, 0)
        FrequencySlider.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        FrequencySlider.Parent = Frame
        
        local FrequencyFill = Instance.new("Frame")
        FrequencyFill.Size = UDim2.new(Settings["Grab"]["Mind Control Frequency"] / 15, 0, 1, 0)
        FrequencyFill.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
        FrequencyFill.Parent = FrequencySlider
        
        local FrequencyButton = Instance.new("TextButton")
        FrequencyButton.Size = UDim2.new(1, 0, 1, 0)
        FrequencyButton.BackgroundTransparency = 1
        FrequencyButton.Text = ""
        FrequencyButton.Parent = FrequencySlider
        
        FrequencyButton.MouseButton1Down:Connect(function()
            local Connection
            Connection = _RunService.RenderStepped:Connect(function()
                local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
                local Percent = (Mouse.X - FrequencySlider.AbsolutePosition.X) / FrequencySlider.AbsoluteSize.X
                Percent = math.clamp(Percent, 0, 1)
                
                FrequencyFill.Size = UDim2.new(Percent, 0, 1, 0)
                local Frequency = math.floor(Percent * 15)
                Settings["Grab"]["Mind Control Frequency"] = Frequency
                FrequencyLabel.Text = "Frequency: " .. Frequency
            end)
            
            local Release
            Release = game:GetService("UserInputService").InputEnded:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 then
                    Connection:Disconnect()
                    Release:Disconnect()
                end
            end)
        end)
        
        -- Cerrar cuando el jugador se quite el ragdoll
        if u19 then
            u19:GetPropertyChangedSignal("Parent"):Connect(function()
                if MindControlUI then
                    MindControlUI:Destroy()
                    MindControlUI = nil
                end
            end)
        end
    end

    -- ===== HERRAMIENTAS DEL GUI =====
    function ToolSSs()
        -- Eliminar herramientas antiguas
        for _, toolName in pairs({
            "Activate", "Throw", "Floar Slam", "Hado 90", "C Moon", "Portal", 
            "Clone", "BoomHeadShot", "Heaven Throw", "Domain Expansion", 
            "Made in Heaven", "Get Over Here", "Head Breaker", "Hell", "Sex",
            "fly punch", "Boxing", "Bat Knock", "Crusher", "Knife Killer",
            "Combo", "Stand", "Rip In Half", "Punch", "Control"
        }) do
            if _LocalPlayer2.Backpack:FindFirstChild(toolName) then
                _LocalPlayer2.Backpack:FindFirstChild(toolName):Destroy()
            end
            if _LocalPlayer2.Character:FindFirstChild(toolName) then
                _LocalPlayer2.Character:FindFirstChild(toolName):Destroy()
            end
        end

        -- ===== TIME STOP (Del GUI) =====
        local TimeStopTool = Instance.new('Tool')
        TimeStopTool.RequiresHandle = false
        TimeStopTool.Name = 'Time Stop'
        TimeStopTool.Parent = _LocalPlayer2.Backpack

        TimeStopTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                -- Detener animaciones de agarre
                AnimStopv2(3135389157)
                AnimStopv2(11075367458)
                
                -- Reproducir sonido de Time Stop
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 5455437798)
                
                -- Animación de carga
                local loadAnim = Instance.new('Animation')
                loadAnim.AnimationId = 'rbxassetid://10714177846'
                local loadTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(loadAnim)
                loadTrack:Play()
                wait(2.5)
                loadTrack:Stop()
                StopAudio()
                
                -- Sonido "When Time is Stopped, There is Only Dio!"
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 6964764258)
                wait(4)
                StopAudio()
                
                -- Conteo
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 6675055864)
                for i = 4, 1, -1 do
                    wait(1.5)
                end
                wait(1)
                StopAudio()
                wait(0.2)
                
                -- Muda Muda Muda
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 6889746326)
                
                -- Animaciones de golpes
                for i = 1, 10 do
                    AnimPlay(8254787838, 9)
                    wait(0.2)
                    AnimStopv2(8254787838)
                    AnimPlay(8254794168, 9)
                    wait(0.2)
                    AnimStopv2(8254794168)
                end
                
                -- Último golpe
                AnimPlay(3354696735, 1.5)
                wait(1)
                
                -- Lanzar jugador
                local bodyPos = GrabbedChar.UpperTorso:FindFirstChildOfClass('BodyPosition')
                if bodyPos then
                    local forwardPos = _LocalPlayer2.Character.HumanoidRootPart.Position + 
                        _LocalPlayer2.Character.HumanoidRootPart.CFrame.LookVector * 19
                    game:GetService('TweenService'):Create(bodyPos, TweenInfo.new(0.8), {Position = forwardPos}):Play()
                end
                
                wait(0.3)
                
                -- Remover partes
                local parts = {"RightUpperLeg", "LeftUpperLeg", "RightUpperArm", "LeftUpperArm"}
                for _, partName in pairs(parts) do
                    if GrabbedChar:FindFirstChild(partName) then
                        GrabbedChar[partName].CFrame = CFrame.new(0, -1200, 0)
                    end
                end
                
                wait(0.3)
                _ReplicatedStorage.MainEvent:FireServer('Grabbing', false)
            end
        end)

        -- ===== GET OVER HERE (Del GUI) =====
        local GetOverHereTool = Instance.new('Tool')
        GetOverHereTool.RequiresHandle = false
        GetOverHereTool.Name = 'Get Over Here'
        GetOverHereTool.Parent = _LocalPlayer2.Backpack

        GetOverHereTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                AnimStopv2(3135389157)
                AnimStopv2(11075367458)
                
                -- Sonido "Get Over Here!"
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 8643750815)
                wait(1.6)
                StopAudio()
                
                -- Sonido de cadena
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 7592607446)
                
                -- Animación
                AnimPlay(16768625968, 1)
                wait(1.1)
                
                -- Traer jugador hacia ti
                local bodyPos = GrabbedChar.UpperTorso:FindFirstChildOfClass('BodyPosition')
                if bodyPos then
                    game:GetService('TweenService'):Create(bodyPos, TweenInfo.new(0.6), {
                        Position = _LocalPlayer2.Character.HumanoidRootPart.Position
                    }):Play()
                end
                
                wait(0.6)
                StopAudio()
            end
        end)

        -- ===== PORTAL (Del GUI) =====
        local PortalTool = Instance.new('Tool')
        PortalTool.RequiresHandle = false
        PortalTool.Name = 'Portal'
        PortalTool.Parent = _LocalPlayer2.Backpack

        PortalTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                if Cache.Loops["Portal"] then return end
                
                local StoredPos = _LocalPlayer2.Character.HumanoidRootPart.CFrame
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                -- Eliminar controles de cuerpo
                for _, vel in pairs(GrabbedChar:GetDescendants()) do
                    if vel:IsA('BodyGyro') or vel:IsA('BodyPosition') then
                        vel:Destroy()
                    end
                end
                
                -- Clonar personaje
                u21 = u107(GrabbedChar)
                u21.Parent = workspace
                u21.HumanoidRootPart.CFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame
                
                Cache.Loops["Portal"] = _RunService.Heartbeat:Connect(function()
                    for _, partName in pairs({
                        'Head', 'HumanoidRootPart', 'UpperTorso', 'LowerTorso',
                        'LeftUpperArm', 'RightUpperArm', 'LeftLowerArm', 'RightLowerArm',
                        'LeftHand', 'RightHand', 'LeftUpperLeg', 'RightUpperLeg',
                        'LeftLowerLeg', 'RightLowerLeg', 'LeftFoot', 'RightFoot'
                    }) do
                        if GrabbedChar:FindFirstChild(partName) and u21:FindFirstChild(partName) then
                            u27(GrabbedChar[partName], u21[partName], CFrame.new())
                        end
                    end
                    
                    ZeroVelocity___(GrabbedChar)
                    
                    -- Hacer que todas las partes no colisionen
                    for _, part in pairs(GrabbedChar:GetChildren()) do
                        if part:IsA('BasePart') then
                            part.CanCollide = false
                        end
                    end
                    
                    _LocalPlayer2.Character.HumanoidRootPart.CFrame = u21.HumanoidRootPart.CFrame - Vector3.new(0, 15, 0)
                    
                    if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                        if Cache.Loops["Portal"] then
                            Cache.Loops["Portal"]:Disconnect()
                            Cache.Loops["Portal"] = nil
                        end
                    end
                end)
                
                workspace.Camera.CameraSubject = u21.Humanoid
                AnimPlay(10714360343, 1, 0, 0.3)
                wait(2)
                AnimStopv2(10714360343)
                
                -- Efecto de cámara
                local camera = workspace.CurrentCamera
                camera.CameraType = Enum.CameraType.Scriptable
                
                local fakePart = Instance.new("Part", workspace)
                fakePart.Size = Vector3.new(1, 1, 1)
                fakePart.Anchored = true
                fakePart.CanCollide = false
                fakePart.Transparency = 1
                fakePart.Position = u21.HumanoidRootPart.Position
                Instance.new("Humanoid", fakePart)
                
                local initialCF = CFrame.new(u21.HumanoidRootPart.Position + Vector3.new(0, 5, 10), u21.HumanoidRootPart.Position)
                local focusCF = CFrame.new(u21.HumanoidRootPart.Position + Vector3.new(0, 2, 30), u21.HumanoidRootPart.Position)
                local headCF = CFrame.new(u21.Head.Position, u21.HumanoidRootPart.Position) + Vector3.new(0, 5, 0)
                
                camera.CFrame = initialCF
                camera.CameraSubject = fakePart
                
                local tweenInfo = TweenInfo.new(2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
                local cameraTween = _TweenService:Create(camera, tweenInfo, {CFrame = focusCF})
                local partTween = _TweenService:Create(fakePart, tweenInfo, {CFrame = focusCF})
                
                cameraTween:Play()
                partTween:Play()
                
                cameraTween.Completed:Wait()
                fakePart:Destroy()
                
                wait(3)
                
                local headTween = _TweenService:Create(camera, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {CFrame = headCF})
                headTween:Play()
                
                headTween.Completed:Wait()
                camera.CameraType = Enum.CameraType.Custom
                camera.CameraSubject = u21.Humanoid
                
                AnimPlay(4940561610, 1, 0, 0.3)
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 7224830040)
                wait(6)
                
                local BodyPos = Instance.new("BodyPosition", GrabbedChar.UpperTorso)
                BodyPos.D = 200
                BodyPos.Position = u21.UpperTorso.Position
                
                AnimStopv2(4940561610)
                
                local NewPart = Instance.new("Part", workspace)
                local Hum = Instance.new("Humanoid", NewPart)
                NewPart.CFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame + Vector3.new(0, 20, 0)
                NewPart.Transparency = 1
                NewPart.Anchored = true
                
                _TweenService:Create(BodyPos, TweenInfo.new(6), {
                    Position = u21.UpperTorso.Position + Vector3.new(0, 20, 0)
                }):Play()
                
                wait(3.5)
                
                -- Void
                local BodyVelocity = Instance.new("BodyVelocity")
                BodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
                BodyVelocity.Velocity = Vector3.new(9e9, -9e9, 9e9)
                BodyVelocity.Parent = GrabbedChar.UpperTorso
                wait(0.1)
                _ReplicatedStorage.MainEvent:FireServer("Grabbing", false)
                
                u21:Destroy()
                if Cache.Loops["Portal"] then
                    Cache.Loops["Portal"]:Disconnect()
                    Cache.Loops["Portal"] = nil
                end
                
                workspace.Camera.CameraSubject = _LocalPlayer2.Character.Humanoid
                
                for i = 1, 40 do
                    _LocalPlayer2.Character.HumanoidRootPart.CFrame = StoredPos
                end
            end
        end)

        -- ===== PUNCH (Del GUI) =====
        local PunchTool = Instance.new('Tool')
        PunchTool.RequiresHandle = false
        PunchTool.Name = 'Punch'
        PunchTool.Parent = _LocalPlayer2.Backpack

        PunchTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                AnimStopv2(3135389157)
                AnimStopv2(11075367458)
                
                -- Simple punch animation
                AnimPlay(3354696735, 1.5)
                wait(0.5)
                
                -- Lanzar jugador
                local bodyPos = GrabbedChar.UpperTorso:FindFirstChildOfClass('BodyPosition')
                if bodyPos then
                    local forwardPos = _LocalPlayer2.Character.HumanoidRootPart.Position + 
                        _LocalPlayer2.Character.HumanoidRootPart.CFrame.LookVector * 15
                    _TweenService:Create(bodyPos, TweenInfo.new(0.5), {Position = forwardPos}):Play()
                end
                
                wait(0.5)
                _ReplicatedStorage.MainEvent:FireServer("Grabbing", false)
            end
        end)

        -- ===== MAKE PLAYER INVISIBLE (Del GUI) =====
        local InvisibleTool = Instance.new('Tool')
        InvisibleTool.RequiresHandle = false
        InvisibleTool.Name = 'Make Player Invisible'
        InvisibleTool.Parent = _LocalPlayer2.Backpack

        InvisibleTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                -- Hacer todas las partes transparentes
                for _, part in pairs(GrabbedChar:GetDescendants()) do
                    if part:IsA('BasePart') then
                        _TweenService:Create(part, TweenInfo.new(0.5), {Transparency = 1}):Play()
                    elseif part:IsA('Decal') then
                        part.Transparency = 1
                    end
                end
                
                -- También hacer transparente la ropa y accesorios
                for _, accessory in pairs(GrabbedChar:GetChildren()) do
                    if accessory:IsA('Accessory') then
                        if accessory:FindFirstChild('Handle') then
                            accessory.Handle.Transparency = 1
                        end
                    end
                end
                
                wait(5) -- Duración de invisibilidad
                
                -- Restaurar visibilidad
                for _, part in pairs(GrabbedChar:GetDescendants()) do
                    if part:IsA('BasePart') then
                        _TweenService:Create(part, TweenInfo.new(0.5), {Transparency = 0}):Play()
                    elseif part:IsA('Decal') then
                        part.Transparency = 0
                    end
                end
            end
        end)

        -- ===== CHEZBURGER (Del GUI) =====
        local ChezburgerTool = Instance.new('Tool')
        ChezburgerTool.RequiresHandle = false
        ChezburgerTool.Name = 'Chezburger'
        ChezburgerTool.Parent = _LocalPlayer2.Backpack

        ChezburgerTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 6647570)
                wait(3)
                
                AnimStopv2(3135389157)
                AnimPlay(2953512033, 1)
                
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 3043029786)
                
                -- Remover partes
                local parts = {"RightUpperLeg", "LeftUpperLeg", "RightUpperArm", "LeftUpperArm"}
                for _, partName in pairs(parts) do
                    local part = _LocalPlayer2.Character.BodyEffects.Grabbed.Value:FindFirstChild(partName)
                    if part then
                        part.CFrame = CFrame.new(0, -1200, 0)
                    end
                end
                
                wait(0.2)
                _ReplicatedStorage.MainEvent:FireServer("Grabbing", false)
                wait(1.5)
                
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 16647579)
            end
        end)

        -- ===== ORBIT (Del GUI) =====
        local OrbitTool = Instance.new('Tool')
        OrbitTool.RequiresHandle = false
        OrbitTool.Name = 'Orbit'
        OrbitTool.Parent = _LocalPlayer2.Backpack

        OrbitTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                if Cache.Loops["Orbit"] then
                    Cache.Loops["Orbit"]:Disconnect()
                    Cache.Loops["Orbit"] = nil
                end
                
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                AnimStopv2(3135389157)
                AnimStopv2(11075367458)
                
                local currentTime = 0
                
                Cache.Loops["Orbit"] = _RunService.Heartbeat:Connect(function(delta)
                    currentTime = currentTime + delta
                    
                    if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                        Cache.Loops["Orbit"]:Disconnect()
                        Cache.Loops["Orbit"] = nil
                        return
                    end
                    
                    local rootPos = _LocalPlayer2.Character.HumanoidRootPart.Position
                    
                    -- Orbitar partes alrededor del jugador
                    local parts = {
                        {name = "UpperTorso", angle = math.pi * 0.1, speed = 1.5},
                        {name = "LowerTorso", angle = math.pi * -0.1, speed = -1.5},
                        {name = "LeftUpperArm", angle = math.pi * .25, speed = 3},
                        {name = "RightUpperArm", angle = math.pi * -.25, speed = -3},
                        {name = "LeftUpperLeg", angle = math.pi * .3, speed = 2.5},
                        {name = "RightUpperLeg", angle = math.pi * -.3, speed = -2.5},
                        {name = "LeftLowerLeg", angle = math.pi * .7, speed = 2},
                        {name = "RightLowerLeg", angle = math.pi * .8, speed = -2},
                        {name = "LeftLowerArm", angle = math.pi * .9, speed = 2},
                        {name = "RightLowerArm", angle = math.pi * 1, speed = -2}
                    }
                    
                    for _, partData in pairs(parts) do
                        local part = GrabbedChar:FindFirstChild(partData.name)
                        if part then
                            part.CFrame = CFrame.Angles(partData.angle, partData.speed * math.pi * currentTime, 0) * 
                                CFrame.new(1, 2, 5) + rootPos
                        end
                    end
                end)
            end
        end)

        -- ===== MADE IN HEAVEN (Del GUI) =====
        local MadeInHeavenTool = Instance.new('Tool')
        MadeInHeavenTool.RequiresHandle = false
        MadeInHeavenTool.Name = 'Made in Heaven'
        MadeInHeavenTool.Parent = _LocalPlayer2.Backpack

        MadeInHeavenTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local old_fov = workspace.CurrentCamera.FieldOfView
                local old_fog = game.Lighting.FogEnd
                local old_fog_color = game.Lighting.FogColor
                
                _LocalPlayer2.Character.Humanoid.WalkSpeed = 0
                AnimStopv2(3135389157)
                
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 8004825017)
                wait(2)
                
                _LocalPlayer2.Character.BodyEffects.Grabbed:GetPropertyChangedSignal("Value"):Connect(function()
                    if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                        _TweenService:Create(workspace.CurrentCamera, TweenInfo.new(1), {FieldOfView = old_fov}):Play()
                        _TweenService:Create(game.Lighting, TweenInfo.new(1), {FogEnd = old_fog}):Play()
                        _TweenService:Create(game.Lighting, TweenInfo.new(1), {FogColor = old_fog_color}):Play()
                        _TweenService:Create(_LocalPlayer2.Character.Humanoid, TweenInfo.new(1), {HipHeight = 2}):Play()
                        AnimStopv2(11394033602)
                    end
                end)
                
                AnimPlay(11394033602, 1)
                
                _TweenService:Create(workspace.CurrentCamera, TweenInfo.new(1), {FieldOfView = 120}):Play()
                _TweenService:Create(game.Lighting, TweenInfo.new(1), {FogEnd = 20}):Play()
                _TweenService:Create(game.Lighting, TweenInfo.new(1), {FogColor = Color3.fromRGB(255,255,255)}):Play()
                _TweenService:Create(_LocalPlayer2.Character.Humanoid, TweenInfo.new(1), {HipHeight = 3.5}):Play()
                
                wait(1)
                
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                local bodyPos = GrabbedChar.UpperTorso:FindFirstChildOfClass('BodyPosition')
                if bodyPos then
                    local forwardPos = _LocalPlayer2.Character.HumanoidRootPart.Position + 
                        _LocalPlayer2.Character.HumanoidRootPart.CFrame.LookVector * 10 + Vector3.new(0, 10, 0)
                    _TweenService:Create(bodyPos, TweenInfo.new(0.5), {Position = forwardPos}):Play()
                end
                
                wait(7)
                
                -- Void
                local BodyVelocity = Instance.new("BodyVelocity")
                BodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
                BodyVelocity.Velocity = Vector3.new(9e9, -9e9, 9e9)
                BodyVelocity.Parent = GrabbedChar.UpperTorso
                wait(0.1)
                _ReplicatedStorage.MainEvent:FireServer("Grabbing", false)
                
                wait(0.8)
                AnimStopv2(11394033602)
                _LocalPlayer2.Character.Humanoid.WalkSpeed = 16
            end
        end)

        -- ===== C MOON (Del GUI) =====
        local CMoonTool = Instance.new('Tool')
        CMoonTool.RequiresHandle = false
        CMoonTool.Name = 'C Moon'
        CMoonTool.Parent = _LocalPlayer2.Backpack

        CMoonTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                _LocalPlayer2.Character.Humanoid.WalkSpeed = 0
                
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 8063279487)
                
                -- Efectos de iluminación
                _TweenService:Create(game.Lighting, TweenInfo.new(0.7), {ColorShift_Bottom = Color3.fromRGB(0,255,0)}):Play()
                _TweenService:Create(game.Lighting, TweenInfo.new(0.7), {ColorShift_Top = Color3.fromRGB(0,255,0)}):Play()
                _TweenService:Create(game.Lighting, TweenInfo.new(0.7), {Brightness = 100}):Play()
                _TweenService:Create(game.Lighting, TweenInfo.new(0.7), {Ambient = Color3.fromRGB(0,255,0)}):Play()
                
                AnimPlay(8254789608, 1)
                
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                local bodyPos = GrabbedChar.UpperTorso:FindFirstChildOfClass('BodyPosition')
                if bodyPos then
                    bodyPos.Position = _LocalPlayer2.Character.HumanoidRootPart.Position + Vector3.new(0, 2000, 0)
                end
                
                wait(3.5)
                
                -- Restaurar iluminación
                _TweenService:Create(game.Lighting, TweenInfo.new(0.7), {ColorShift_Bottom = Color3.fromRGB(0,0,0)}):Play()
                _TweenService:Create(game.Lighting, TweenInfo.new(0.7), {ColorShift_Top = Color3.fromRGB(0,0,0)}):Play()
                _TweenService:Create(game.Lighting, TweenInfo.new(0.7), {Brightness = 1}):Play()
                _TweenService:Create(game.Lighting, TweenInfo.new(0.7), {Ambient = Color3.fromRGB(0,0,0)}):Play()
                
                _LocalPlayer2.Character.Humanoid.WalkSpeed = 16
            end
        end)

        -- ===== HADO 90 (Del GUI) =====
        local Hado90Tool = Instance.new('Tool')
        Hado90Tool.RequiresHandle = false
        Hado90Tool.Name = 'Hado 90'
        Hado90Tool.Parent = _LocalPlayer2.Backpack

        Hado90Tool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 4743237673)
                wait(2.5)
                
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 18641380807)
                
                -- Efecto visual
                local Part = Instance.new("Part")
                Part.Anchored = true
                Part.BottomSurface = Enum.SurfaceType.Smooth
                Part.CanCollide = false
                Part.TopSurface = Enum.SurfaceType.Smooth
                Part.Color = Color3.fromRGB(17, 17, 17)
                Part.Material = Enum.Material.Neon
                Part.Size = Vector3.new(9, 25, 9)
                
                local Highlight = Instance.new("Highlight")
                Highlight.OutlineColor = Color3.fromRGB(170, 0, 255)
                Highlight.FillColor = Color3.fromRGB(0, 0, 0)
                Highlight.Parent = Part
                Part.Parent = workspace
                
                Part.CFrame = CFrame.new(GrabbedChar.UpperTorso.Position + Vector3.new(0, -50, 0))
                
                _TweenService:Create(Part, TweenInfo.new(1), {
                    CFrame = CFrame.new(GrabbedChar.UpperTorso.Position + Vector3.new(0, 5, 0))
                }):Play()
                
                wait(5)
                
                _TweenService:Create(Part, TweenInfo.new(1), {
                    CFrame = CFrame.new(GrabbedChar.UpperTorso.Position + Vector3.new(0, 50, 0))
                }):Play()
                
                -- Void
                local BodyVelocity = Instance.new("BodyVelocity")
                BodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
                BodyVelocity.Velocity = Vector3.new(9e9, -9e9, 9e9)
                BodyVelocity.Parent = GrabbedChar.UpperTorso
                wait(0.1)
                _ReplicatedStorage.MainEvent:FireServer("Grabbing", false)
                
                wait(1)
                Part:Destroy()
            end
        end)

        -- ===== SEX (Del GUI con velocidad ajustable) =====
        local SexTool = Instance.new('Tool')
        SexTool.RequiresHandle = false
        SexTool.Name = 'Sex'
        SexTool.Parent = _LocalPlayer2.Backpack

        SexTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                -- Mostrar UI para ajustar velocidad
                CreateSexSpeedUI()
                
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                AnimPlay(4784557631, Settings["Grab"]["Sex Speed"] or 10)
                AnimPlay(3119980985, 2)
                
                while _LocalPlayer2.Character.BodyEffects.Grabbed.Value == GrabbedChar do
                    wait()
                    
                    -- Mover al jugador agarrado
                    local bodyPos = GrabbedChar.UpperTorso:FindFirstChildOfClass('BodyPosition')
                    if bodyPos then
                        local tickTime = tick() * 133.33
                        local offset = CFrame.new(0, -0.3, -2 + 0.5 * math.sin(tickTime / 8))
                        bodyPos.Position = (_LocalPlayer2.Character.HumanoidRootPart.CFrame * offset).Position
                    end
                    
                    -- Asegurar que no colisione
                    for _, part in pairs(GrabbedChar:GetChildren()) do
                        if part:IsA('BasePart') then
                            part.CanCollide = false
                        end
                    end
                end
                
                -- Limpiar
                AnimStopv2(4784557631)
                AnimStopv2(3119980985, 0.5)
                
                -- Cerrar UI si está abierta
                if SexSpeedUI then
                    SexSpeedUI:Destroy()
                    SexSpeedUI = nil
                end
            end
        end)

        -- ===== DOMAIN EXPANSION (Del GUI) =====
