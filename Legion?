    local function u84(p73)
        local v74, v75, v76 = pairs(_LocalPlayer2.Character.Humanoid:GetPlayingAnimationTracks())

        while true do
            local v77

            v76, v77 = v74(v75, v76)

            if v76 == nil then
                break
            end
            if v77.Animation.AnimationId:match('rbxassetid://' .. p73) then
                v77:Stop()

                if not u21:FindFirstChild(p73) then
                    local _Animation3 = Instance.new('Animation', u21)

                    _Animation3.AnimationId = 'rbxassetid://' .. p73
                    _Animation3.Name = '_____GYAT'
                    playing = u21.Humanoid:LoadAnimation(_Animation3)

                    playing:Play()
                end
            end
        end

        if u21.Humanoid.MoveDirection.magnitude > 0 then
            CloneStopanim(p73)

            local v79 = u21
            local v80, v81, v82 = pairs(v79:GetChildren())

            while true do
                local v83

                v82, v83 = v80(v81, v82)

                if v82 == nil then
                    break
                end
                if v83.Name == '_____GYAT' then
                    v83:Destroy()
                end
            end
        end
    end
    
