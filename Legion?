                if bodyPos then
                    local forwardPos = _LocalPlayer2.Character.HumanoidRootPart.Position + 
                        _LocalPlayer2.Character.HumanoidRootPart.CFrame.LookVector * 10
                    _TweenService:Create(bodyPos, TweenInfo.new(0.5), {Position = forwardPos}):Play()
                end
                
                loaded_Animation.Stopped:Wait()
                
                -- Void
                local BodyVelocity = Instance.new("BodyVelocity")
                BodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
                BodyVelocity.Velocity = Vector3.new(9e9, -9e9, 9e9)
                BodyVelocity.Parent = GrabbedChar.UpperTorso
                wait(0.1)
                _ReplicatedStorage.MainEvent:FireServer("Grabbing", false)
                
                _TweenService:Create(_LocalPlayer2.Character.Humanoid, TweenInfo.new(1), {HipHeight = 2.008}):Play()
                _LocalPlayer2.Character.Humanoid.WalkSpeed = 16
            end
        end)

        -- ===== MINION (Nuevo) =====
        local MinionTool = Instance.new('Tool')
        MinionTool.RequiresHandle = false
        MinionTool.Name = 'Minion'
        MinionTool.Parent = _LocalPlayer2.Backpack

        MinionTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                -- Sonido de Minions
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 5986485937)
                
                -- Cambiar color a amarillo (como los Minions)
                for _, part in pairs(GrabbedChar:GetChildren()) do
                    if part:IsA('BasePart') then
                        _TweenService:Create(part, TweenInfo.new(0.5), {
                            Color = Color3.fromRGB(255, 255, 0)
                        }):Play()
                    end
                end
                
                -- Animación tonta
                local danceAnim = Instance.new('Animation')
                danceAnim.AnimationId = 'rbxassetid://507770818'
                local danceTrack = GrabbedChar.Humanoid:LoadAnimation(danceAnim)
                danceTrack:Play()
                danceTrack:AdjustSpeed(1.5)
                
                wait(3)
                
                -- Restaurar color
                for _, part in pairs(GrabbedChar:GetChildren()) do
                    if part:IsA('BasePart') then
                        _TweenService:Create(part, TweenInfo.new(0.5), {
                            Color = Color3.fromRGB(255, 255, 255)
                        }):Play()
                    end
                end
                
                danceTrack:Stop()
            end
        end)

        -- ===== MAKE PLAYER STAND (Nuevo) =====
        local MakeStandTool = Instance.new('Tool')
        MakeStandTool.RequiresHandle = false
        MakeStandTool.Name = 'Make Player Stand'
        MakeStandTool.Parent = _LocalPlayer2.Backpack

        MakeStandTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                -- Crear clon para hacer stand
                u21 = u107(GrabbedChar)
                
                AnimStopv2(3135389157)
                AnimStopv2(11075367458)
                
                -- Eliminar controles de cuerpo del original
                for _, part in pairs(GrabbedChar:GetDescendants()) do
                    if part:IsA('BodyPosition') or part:IsA('BodyGyro') then
                        part:Destroy()
                    end
                end
                
                Cache.Loops["Make Player Stand"] = _RunService.Heartbeat:Connect(function()
                    if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                        if Cache.Loops["Make Player Stand"] then
                            Cache.Loops["Make Player Stand"]:Disconnect()
                            Cache.Loops["Make Player Stand"] = nil
                        end
                        u21:Destroy()
                        return
                    end
                    
                    -- Alinear partes con el clon
                    local parts = {
                        'Head', 'HumanoidRootPart', 'UpperTorso', 'LowerTorso',
                        'LeftUpperArm', 'RightUpperArm', 'LeftLowerArm', 'RightLowerArm',
                        'LeftHand', 'RightHand', 'LeftUpperLeg', 'RightUpperLeg',
                        'LeftLowerLeg', 'RightLowerLeg', 'LeftFoot', 'RightFoot'
                    }
                    
                    for _, partName in pairs(parts) do
                        if GrabbedChar:FindFirstChild(partName) and u21:FindFirstChild(partName) then
                            u27(GrabbedChar[partName], u21[partName], CFrame.new())
                        end
                    end
                    
                    -- Posicionar el clon detrás del jugador
                    local rootPart = _LocalPlayer2.Character.HumanoidRootPart
                    u21.HumanoidRootPart.CFrame = CFrame.new(
                        rootPart.Position + rootPart.CFrame.LookVector * -3 + 
                        rootPart.CFrame.RightVector * -1 + 
                        Vector3.new(0, 3.5, 0)
                    )
                    
                    -- Animación de idle
                    local idleAnim = Instance.new('Animation')
                    idleAnim.AnimationId = 'rbxassetid://3541114300'
                    local idleTrack = u21.Humanoid:LoadAnimation(idleAnim)
                    idleTrack:Play()
                end)
                
                AnimPlay(3541114300, 1, 0, 0.3)
            end
        end)

