                            u21.Humanoid.WalkSpeed = 24
                        elseif p115 == 'c' then
                            _G.FlyClone = not _G.FlyClone
                        elseif p115 == 'x' then
                            u114 = not u114

                            if u114 == true then
                                u92(3189777795)

                                repeat
                                    task.wait()

                                    u21.HumanoidRootPart.CFrame = u21.HumanoidRootPart.CFrame + u21.Humanoid.MoveDirection * getgenv().config.control.cframespeed
                                until u114 == false
                            end
                        end
                    end))
                    task.spawn(function()
                        while true do
                            repeat
                                task.wait(0.2)
                            until _LocalPlayer2 and _LocalPlayer2.Character and _LocalPlayer2.Character:FindFirstChild('BodyEffects')

                            local _Grabbed = _LocalPlayer2.Character.BodyEffects:FindFirstChild('Grabbed')

                            if _Grabbed and _Grabbed.Value == nil then
                                _G.FlyClone = false
                            end
                        end
                    end)
                    u31(u22, _LocalPlayer2:GetMouse().KeyUp:Connect(function(p117)
                        if p117 == '2' then
                            CloneStopanim(3152394906)
                        elseif p117 == 'f' then
                            CloneStopanim(2788354405)
                        elseif p117 == '0' then
                            u21.Humanoid.WalkSpeed = 16
                        end
                    end))
                    u31(u22, _LocalPlayer2.Backpack.Combat.Activated:Connect(function(_)
                        u100(u28[math.random(1, #u28)], 1.3)
                    end))

                    repeat
                        task.wait()
                    until _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil or u20 == true

                    _LocalPlayer2.Character.HumanoidRootPart.CFrame = u21.HumanoidRootPart.CFrame

                    ZeroVelocity___(_LocalPlayer2.Character.HumanoidRootPart)

                    workspace.CurrentCamera.CameraSubject = _LocalPlayer2.Character

                    u21:Destroy()

                    local v118 = u19
                    local v119, v120, v121 = pairs(v118:GetDescendants())

                    while true do
                        local v122

                        v121, v122 = v119(v120, v121)

                        if v121 == nil then
                            break
                        end
                        if v122:IsA('BodyPosition') then
                            v122:Destroy()
                        end
                    end

                    local v123, v124, v125 = pairs(u22)

                    while true do
                        local v126

                        v125, v126 = v123(v124, v125)

                        if v125 == nil then
                            break
                        end

                        v126:Disconnect()
                    end

                    u19.LeftHand:FindFirstChildOfClass('Motor6D').Enabled = true
                    u19.RightHand:FindFirstChildOfClass('Motor6D').Enabled = true
                    u19.LeftFoot:FindFirstChildOfClass('Motor6D').Enabled = true
                    u19.RightFoot:FindFirstChildOfClass('Motor6D').Enabled = true
                end
                
                u127()
            end
        end)

        -- Herramientas básicas de Zeraya (Activación y agarre)
        local ActivateTool = Instance.new('Tool')
        ActivateTool.RequiresHandle = false
        ActivateTool.Name = 'Activate'
        ActivateTool.Parent = _LocalPlayer2.Backpack

        ActivateTool.Activated:Connect(function()
            game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', true)
            wait(0.3)
            
            -- Función Grab de Zeraya
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                u19 = _LocalPlayer2.Character.BodyEffects.Grabbed.Value

                local _GRABBING_CONSTRAINT = u19:WaitForChild('GRABBING_CONSTRAINT')

                if not _GRABBING_CONSTRAINT then
                    return
                end

                u23 = _GRABBING_CONSTRAINT:FindFirstChildOfClass('RopeConstraint')
                u23.Length = 1 / 0
                u20 = false

                local v146 = u19
                local v147, v148, v149 = pairs(v146:GetChildren())

                while true do
                    local v150

                    v149, v150 = v147(v148, v149)

                    if v149 == nil then
                        break
                    end
                    if v150:IsA('BasePart') then
                        v150.CanCollide = false
                    end
                end
                while true do
                    task.wait()

                    local v151, v152, v153 = pairs(_LocalPlayer2.Character.Humanoid:GetPlayingAnimationTracks())

                    while true do
                        local v154

                        v153, v154 = v151(v152, v153)

                        if v153 == nil then
                            break
                        end
                        if v154.Animation.AnimationId:match('rbxassetid://11075367458') then
                            u20 = true

                            v154:Stop()
                        end
                    end

                    if u20 == true then
                        local v155 = u19
                        local v156, v157, v158 = pairs(v155:GetDescendants())

                        while true do
                            local v159

                            v158, v159 = v156(v157, v158)

                            if v158 == nil then
                                break
                            end
                            if v159:IsA('BasePart') then
                                v159.Velocity = Vector3.zero
                                v159.AssemblyAngularVelocity = Vector3.zero
                                v159.AssemblyLinearVelocity = Vector3.zero
                            end
                        end

                        u20 = false

                        AnimPlayvv2(3135389157, 0, 0.1, 0.3)
                        
                        -- Holding function
                        repeat
                            task.wait()
                            -- Align function para mantener agarrado
                            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value ~= nil then
                                if not (u19.UpperTorso:FindFirstChildWhichIsA('BodyPosition') or u19.UpperTorso:FindFirstChildWhichIsA('BodyGyro')) then
                                    Instance.new('BodyPosition', u19.UpperTorso).MaxForce = Vector3.new(math.huge, math.huge, math.huge)

                                    local _BodyGyro = Instance.new('BodyGyro', u19.UpperTorso)

                                    _BodyGyro.maxTorque = Vector3.new(9000000000, 9000000000, 9000000000)
                                    _BodyGyro.P = 10000
                                end

                                u19.UpperTorso.BodyGyro.CFrame = _LocalPlayer2.Character.RightUpperArm.CFrame * CFrame.Angles(math.pi * -0.5, math.pi * 1, math.pi * 0.07)
                                u19.UpperTorso.BodyPosition.Position = _LocalPlayer2.Character.RightUpperArm.CFrame * CFrame.new(-0.2, -1.7, 0.5).Position
                                u19.UpperTorso.BodyPosition.P = 10000
                                u19.UpperTorso.BodyPosition.D = 175
                            end
                        until _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil or u20 == true
                        
                        AnimStopv2(3135389157)
                        
                        -- Destroy function
                        if u19.UpperTorso:FindFirstChildOfClass('BodyGyro') or u19.UpperTorso:FindFirstChildOfClass('BodyPosition') then
                            u19.UpperTorso:FindFirstChildOfClass('BodyGyro'):Destroy()
                            u19.UpperTorso:FindFirstChildOfClass('BodyPosition'):Destroy()
                        end
                    end
                end
            else
                return
            end
        end)

        local ThrowTool = Instance.new('Tool')
        ThrowTool.RequiresHandle = false
        ThrowTool.Name = 'Throw'
        ThrowTool.Parent = _LocalPlayer2.Backpack

        ThrowTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', false)
            end
        end)
    end

    ToolSSs()
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        game.Players.LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR')
        ToolSSs()
    end)
else
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Your Not Buyer',
        Text = 'Please Buy Our Script Our Discord Server (Discord Server Set To Your CilpBoard)',
        Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
        Duration = 10,
    })
    setclipboard('https://discord.gg/mFnYMm7e')
end
