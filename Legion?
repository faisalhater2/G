    local function u37(p32, p33)
        if game:GetService('RunService')[p32] then
            local u34 = nil

            u34 = game:GetService('RunService')[p32]:Connect(function()
                local v35, v36 = pcall(p33)

                if not v35 then
                    u34:Disconnect()
                    warn(v36)
                end
            end)

            return u34
        end
    end

    function ZeroVelocity___(p38)
