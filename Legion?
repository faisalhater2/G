                -- Sistema de controles (simplificado del GUI)
                local function u127()
                    local u114 = nil

                    u31(u22, _LocalPlayer2:GetMouse().KeyDown:Connect(function(p115)
                        if p115 == 'v' then
                            u92(7024352298)

                            repeat
                                wait()
