        -- ===== MIND CONTROL (Nuevo con ajustes) =====
        local MindControlTool = Instance.new('Tool')
        MindControlTool.RequiresHandle = false
        MindControlTool.Name = 'Mind Control'
        MindControlTool.Parent = _LocalPlayer2.Backpack

        MindControlTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                -- Mostrar UI para ajustes
                CreateMindControlUI()
                
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                -- Crear efecto de control mental (rayos)
                local Attachments = {}
                local Beams = {}
                
                -- Crear attachments en el jugador
                for _, partName in pairs({'Head', 'UpperTorso', 'LowerTorso'}) do
                    local part = _LocalPlayer2.Character:FindFirstChild(partName)
                    local targetPart = GrabbedChar:FindFirstChild(partName)
                    
                    if part and targetPart then
                        local attachment1 = Instance.new("Attachment")
                        attachment1.Parent = part
                        attachment1.Position = Vector3.new(0, 0, 0)
                        
                        local attachment2 = Instance.new("Attachment")
                        attachment2.Parent = targetPart
                        attachment2.Position = Vector3.new(0, 0, 0)
                        
                        local beam = Instance.new("Beam")
                        beam.Attachment0 = attachment1
                        beam.Attachment1 = attachment2
                        beam.Color = ColorSequence.new(Settings["Grab"]["Mind Control Color"])
                        beam.Width0 = Settings["Grab"]["Mind Control Thickness"]
                        beam.Width1 = Settings["Grab"]["Mind Control Thickness"]
                        beam.Parent = workspace
                        
                        table.insert(Attachments, attachment1)
                        table.insert(Attachments, attachment2)
                        table.insert(Beams, beam)
                    end
                end
                
                -- Efecto de sonido psíquico
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 4580050667) -- "IMMORTALITY, ETERNAL STANDO POWER!"
                
                -- Controlar al jugador mientras esté agarrado
                Cache.Loops["Mind Control"] = _RunService.Heartbeat:Connect(function()
                    if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                        -- Limpiar
                        for _, attachment in pairs(Attachments) do
                            attachment:Destroy()
                        end
                        for _, beam in pairs(Beams) do
                            beam:Destroy()
                        end
                        if Cache.Loops["Mind Control"] then
                            Cache.Loops["Mind Control"]:Disconnect()
                            Cache.Loops["Mind Control"] = nil
                        end
                        if MindControlUI then
                            MindControlUI:Destroy()
                            MindControlUI = nil
                        end
                        return
                    end
                    
                    -- Actualizar beams con ajustes
                    for _, beam in pairs(Beams) do
                        beam.Color = ColorSequence.new(Settings["Grab"]["Mind Control Color"])
                        beam.Width0 = Settings["Grab"]["Mind Control Thickness"]
                        beam.Width1 = Settings["Grab"]["Mind Control Thickness"]
                        
                        -- Efecto de frecuencia (parpadeo)
                        if math.random(1, 15) <= Settings["Grab"]["Mind Control Frequency"] then
                            beam.Transparency = NumberSequence.new(0.5)
                        else
                            beam.Transparency = NumberSequence.new(0)
                        end
                    end
                    
                    -- Hacer que el jugador siga al usuario
                    local bodyPos = GrabbedChar.UpperTorso:FindFirstChildOfClass('BodyPosition')
                    if bodyPos then
                        bodyPos.Position = _LocalPlayer2.Character.HumanoidRootPart.Position + 
                            _LocalPlayer2.Character.HumanoidRootPart.CFrame.LookVector * 3
                    end
                end)
                
                wait(10) -- Duración del control mental
                
                -- Limpiar al final
                for _, attachment in pairs(Attachments) do
                    attachment:Destroy()
                end
                for _, beam in pairs(Beams) do
                    beam:Destroy()
                end
                if Cache.Loops["Mind Control"] then
                    Cache.Loops["Mind Control"]:Disconnect()
                    Cache.Loops["Mind Control"] = nil
                end
                if MindControlUI then
                    MindControlUI:Destroy()
                    MindControlUI = nil
                end
            end
        end)

        -- ===== HOVERBOARD (Nuevo) =====
        local HoverboardTool = Instance.new('Tool')
        HoverboardTool.RequiresHandle = false
        HoverboardTool.Name = 'Hoverboard'
        HoverboardTool.Parent = _LocalPlayer2.Backpack

        HoverboardTool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local GrabbedChar = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                -- Crear efecto de hoverboard
                local Hoverboard = Instance.new("Part")
                Hoverboard.Name = "Hoverboard"
                Hoverboard.Size = Vector3.new(4, 0.5, 8)
                Hoverboard.Color = Color3.fromRGB(0, 170, 255)
                Hoverboard.Material = Enum.Material.Neon
                Hoverboard.Anchored = false
                Hoverboard.CanCollide = false
                Hoverboard.Parent = GrabbedChar
                
                local Weld = Instance.new("Weld")
                Weld.Part0 = GrabbedChar.LowerTorso
                Weld.Part1 = Hoverboard
                Weld.C0 = CFrame.new(0, -1, 0)
                Weld.Parent = Hoverboard
                
                -- Efectos de partículas
                local Particles = {}
                for i = -1, 1, 2 do
                    for j = -1, 1, 2 do
                        local ParticleEmitter = Instance.new("ParticleEmitter")
                        ParticleEmitter.Parent = Hoverboard
                        ParticleEmitter.Size = NumberSequence.new(0.5)
                        ParticleEmitter.Transparency = NumberSequence.new(0.5)
                        ParticleEmitter.Lifetime = NumberRange.new(0.5)
                        ParticleEmitter.Rate = 50
                        ParticleEmitter.Speed = NumberRange.new(5)
                        ParticleEmitter.VelocitySpread = 10
                        ParticleEmitter.Color = ColorSequence.new(Color3.fromRGB(0, 170, 255))
                        ParticleEmitter.LightEmission = 0.5
                        ParticleEmitter.Acceleration = Vector3.new(0, 10, 0)
                        
                        local Attachment = Instance.new("Attachment")
                        Attachment.Parent = Hoverboard
                        Attachment.Position = Vector3.new(i * 1.5, -0.25, j * 3)
                        ParticleEmitter.Attachment = Attachment
                        
                        table.insert(Particles, {Emitter = ParticleEmitter, Attachment = Attachment})
                    end
                end
                
                -- Sonido de hoverboard
                _ReplicatedStorage.MainEvent:FireServer("Boombox", 7224830040)
                
                -- Elevar al jugador
                local bodyPos = GrabbedChar.UpperTorso:FindFirstChildOfClass('BodyPosition')
                if bodyPos then
                    bodyPos.Position = bodyPos.Position + Vector3.new(0, 5, 0)
                end
                
                wait(5) -- Duración del hoverboard
                
                -- Limpiar
                Hoverboard:Destroy()
                for _, particleData in pairs(Particles) do
                    particleData.Emitter:Destroy()
                    particleData.Attachment:Destroy()
                end
                StopAudio()
            end
        end)

        -- ===== CONTROL (Del GUI - versión simplificada) =====
        local ControlTool = Instance.new('Tool')
